<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
                        
<mapper namespace="com.kosta.jupjup.dao.ServiceCenterDAO">
	
	<select id="getallList" resultType="com.kosta.jupjup.vo.ServiceCenterVO">
		<![CDATA[
			SELECT * FROM QUESTIONS where rownum<=10
		]]>
	</select>
	
	<select id="getnoticelist" resultType="com.kosta.jupjup.vo.NoticeVO">
		<![CDATA[
			SELECT * FROM NOTICE where rownum<=10
		]]>
	</select>
</mapper>		